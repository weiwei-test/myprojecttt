radient(0, 0, 0, 1, [{\n                  offset: 0, \n                  color: '#9c6b4e'\n                },{\n                  offset: 1, \n                  color: '#2a59ac'\n                }]),\n                lineStyle: {\n                  color: '#2a59ac'\n                }\n              }\n            }\n          },\n          {\n            value: [70, 40, 55, 55, 30, 55],\n            name: '5G手机',\n            itemStyle: {\n              normal: {\n                color: new echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n                  offset: 0, \n                  color: '#0855ca'\n                },{\n                  offset: 1, \n                  color: '#36a6c7'\n                }]),\n                lineStyle: {\n                  color: '#36a6c7'\n                }\n              }\n            }\n          }]\n        }]\n      }\n\n      myChart.setOption(this.option, true);\n\n      window.addEventListener('resize', () => {\n        myChart.resize();\n      });\n    }\n  },\n  beforeDestroy() {\n    \n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.sn-container {\n  left: 1436px;\n  top: 1978px;\n  width: 432px;\n  height: 400px;\n  .chartsdom {\n    width: 100%;\n    height: 100%;\n  }\n}\n</style>\n"]}]}