Jack Chen\n 日期: 2020-05-03\n-->\n\n<template>\n  <div class=\"wrap-container sn-container\"> \n    <div class=\"sn-content\"> \n      <div class=\"sn-title\">水球图</div> \n      <div class=\"sn-body\"> \n        <div class=\"wrap-container\"> \n          <div class=\"chartsdom\" id=\"chart_polo\"></div> \n        </div> \n      </div> \n    </div>   \n  </div>\n</template>\n\n<script>\n\n\nexport default {\n  name: \"waterPolo\",\n  data() {\n    return {\n      option: null\n      \n    }\n  },\n  mounted() {\n    this.getEchart();\n  },\n  methods: {\n    getEchart() {\n      let myChart = echarts.init(document.getElementById('chart_polo'));\n      this.option = {\n        series: [{\n          type: 'liquidFill',\n          data: [0.45],\n          radius: '70%', \n          color: ['#00b9f5'],\n          backgroundStyle: {\n            color: 'rgba(0, 0, 0, 0.5)',\n            borderColor: '#007bff',\n            borderWidth: 3,\n            shadowColor: 'rgba(0, 123, 225, 0.4)',\n            shadowBlur: 20\n          },\n          outline: {\n            show: false,\n          }, \n        }]\n      }\n\n      myChart.setOption(this.option, true);\n\n      window.addEventListener('resize', () => {\n        myChart.resize();\n      });\n    }\n  },\n  beforeDestroy() {\n    \n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.sn-container {\n  left: 974px;\n  top: 2838px;\n  width: 432px;\n  height: 400px;\n  .chartsdom {\n    width: 100%;\n    height: 100%;\n  }\n}\n</style>\n"]}]}